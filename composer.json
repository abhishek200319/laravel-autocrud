{
    "name": "abhishek-himanshu-04/laravel-autocrud",
    "description": "A package to create automatic crud api for a specific resource",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Api\\LaravelAutocrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Api\\LaravelAutocrud\\Tests\\Feature\\": "tests/Feature/"
        }
    },
    "authors": [
        {
            "name": "abhishek dixit",
            "email": "abhishekdixit342@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Api\\LaravelAutocrud\\AutoCrudServiceProvider"
            ]
        }
    },
    
    "prefer-stable": true
}
